{
  "name": "pro-e-shop",
  "version": "1.0.0",
  "description": "e-commerce platform with MERN stack",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "server": "nodemon backend/server",
    "client": "npm start --prefix frontend",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "test": "echo \"Error: no test specified\" && exit 1",
    "data:import": "node backend/seeder",
    "data:clear": "node backend/seeder -d",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix frontend && npm run build --prefix frontend"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ramanarao1997/E-commerce-platform-with-MERN.git"
  },
  "author": "Ramana Rao Akula",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ramanarao1997/E-commerce-platform-with-MERN/issues"
  },
  "homepage": "https://github.com/ramanarao1997/E-commerce-platform-with-MERN#readme",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "colors": "^1.4.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-async-handler": "^1.1.4",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.10.9",
    "path": "^0.12.7"
  },
  "devDependencies": {
    "concurrently": "^5.3.0",
    "nodemon": "^2.0.4"
  }
}